;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Thesis Terraformer"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[editor_plugins]

enabled=PackedStringArray("res://addons/terraformer/plugin.cfg")

[plugins]

ai_assistant_hub/llm_api="gemini_api"
ai_assistant_hub/preferences/thinking_target=0
ai_assistant_hub/preferences/skip_greeting=false
ai_assistant_hub/preferences/always_scroll_to_bottom=false

[rendering]

textures/canvas_textures/default_texture_filter=0
